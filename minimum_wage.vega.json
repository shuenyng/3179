{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/shuenyng/3179/main/data/world-data-2023.csv",
    "format": {
      "type": "csv"
    }
  },
  "mark": {
    "type": "point",
    "filled": true,
    "size": 100
  },
  "encoding": {
    "x": {
      "field": "Continent",
      "type": "ordinal",
      "axis": {"title": "Continent", "labelAngle": -360}
    },
    "y": {
      "field": "Minimum Wage",
      "type": "quantitative",
      "axis": {"title": "Minimum Wage"}
    },
    "color": {
      "field": "Continent",
      "type": "nominal",
      "scale": {"scheme": "category20"}
    },
    "tooltip": [
      {"field": "Continent", "title": "Continent"},
      {"field": "Minimum Wage", "title": "Minimum Wage"}
    ]
  },
  "title": "Minimum Wage by Continent",
  "width": 400,
  "height": 300
}
