{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "https://raw.githubusercontent.com/shuenyng/3179/main/data/world-data-2023.csv"},
  "mark": {
    "type": "boxplot",
    "extent": "min-max"
  },
  "encoding": {
    "x": {"field": "Continent", "type": "nominal", "axis": {"labelAngle": -360}},
    "y": {
      "field": "Gasoline Price",
      "type": "quantitative",
      "scale": {"zero": false}
    },
    "color": {"field": "Continent", "type": "nominal", "scale": {"scheme": "category20"}}
  },
  "title": "Boxplot of Gasoline Prices",
  "width": 600,
  "height": 400
}
